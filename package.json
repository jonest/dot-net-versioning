  "license": "MIT",
  "devDependencies": {
    "@types/node": "7.0.27",
    "@types/shelljs": "0.7.2",
    "rimraf": "2.6.1",
    "tfx-cli": "0.4.8",
    "typescript": "2.3.4"
  }
}
